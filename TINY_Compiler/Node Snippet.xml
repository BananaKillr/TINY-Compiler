<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippet Format="1.0.0">
	<Header>
		<Title>Create Node Function</Title>
		<Shortcut>nodefunc</Shortcut>
		<Description>Generates a function for creating a Node object</Description>
		<Author>YourName</Author>
		<SnippetTypes>
			<SnippetType>Expansion</SnippetType>
		</SnippetTypes>
	</Header>
	<Snippet>
		<Declarations>
			<Literal>
				<ID>FunctionName</ID>
				<ToolTip>Name of the function</ToolTip>
				<Default>DataType</Default>
			</Literal>
		</Declarations>
		<Code Language="csharp">
			<![CDATA[
     Node $FunctionName$()
     {
         Node $lowercase(FunctionName)$ = new Node("$FunctionName$");
         return $lowercase(FunctionName)$;
     }
      ]]>
		</Code>
	</Snippet>
	</CodeSnippet>
